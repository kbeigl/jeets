    </parent>

	<artifactId>jeets-parent</artifactId>
	<packaging>pom</packaging>
	<name>JeeTS Parent</name>
	<description>
		Parent POM to compile and test all JeeTS sources with
		dependencies.
	</description>
	<url>http://www.jeets.org/</url>

	<licenses>
		<license>
			<name>Apache License, Version 2.0</name>
			<distribution>repo</distribution>
			<url>http://www.apache.org/licenses/LICENSE-2.0.html</url>
		</license>
	</licenses>

	<prerequisites>
		<maven>3.0</maven>
	</prerequisites>

	<properties>
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>

		<!-- Adjust with Camel parent with listed versions -->
		<slf4j-version>1.7.22</slf4j-version>
		<commons-logging-version>1.2</commons-logging-version>
		<junit-version>4.12</junit-version>

		<!-- move down to persistence ? -->
		<hibernate-version>5.2.4.Final</hibernate-version>
		<h2database-version>1.3.176</h2database-version>
		<postgresql-version>9.4-1200-jdbc41</postgresql-version>
		<netty-version>4.1.15.Final</netty-version>
	</properties>

